#include <iostream>
#include <vector>
#include <queue>
#include <functional>
using namespace std;

void solve()//список смежности
{
    int n; cin >> n;
    vector<vector<int>> abj(n + 1);
    for (int i = 0; i < n; ++i) {
        int u, v;
        cin >> u >> v;
        abj[u].push_back(v);
        abj[v].push_back(u);
    }

    //поиск в глубину
    vector<int> used(n + 1);
    std::function<void(int)> dfs = [&](int u) {
        used[u] = 1;

        for (auto& next : abj[u]) {
            if (!used[next])
            {
                dfs(next);
            }
        }
    };
    dfs(1);
}

int main()
{
    int t = 1;
    while (t--) solve();
    return 0;
}
